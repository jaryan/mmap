useDynLib(mmap)

# user functions
export(
       mmap,
       munmap,
       msync,
       mprotect,
       
       mmapFlags,

       is.mmap,

       extractFUN,
       "extractFUN<-",
       replaceFUN,
       "replaceFUN<-",

       char, int8,  int16,  int32,  real32,  real64,
             uint8, uint16,
       struct,

       as.Ctype
      )

# S3 methods
S3method('[',mmap)
S3method('[<-',mmap)
S3method(extractFUN,mmap)
S3method('extractFUN<-',mmap)
S3method(replaceFUN,mmap)
S3method('replaceFUN<-',mmap)
S3method(close,mmap)
S3method(str,mmap)
S3method(print,mmap)
S3method(length, mmap)
#S3method(head,mmap)
#S3method(tail,mmap)
S3method(as.Ctype, Ctype)
S3method(as.Ctype, integer)
S3method(as.Ctype, double)
S3method(as.Ctype, raw)
S3method(as.Ctype, character)


S3method(as.list, Ctype)
